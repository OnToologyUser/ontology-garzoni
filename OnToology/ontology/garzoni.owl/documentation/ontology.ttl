      rdfs:range <http://vocab.dhlab.epfl.ch/garzoni#ProfessionMention> ;
      rdfs:subPropertyOf <http://vocab.dhlab.epfl.ch/garzoni#personMentionProperties> .

<http://vocab.dhlab.epfl.ch/garzoni#ProfessionMention>
